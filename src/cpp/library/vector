  void assign(void *t1, void *t2)
  {
    int i = 0;
    _size = 0;
    T *t11 = (T *)t1;
    T *t21 = (T *)t2;
    while ((t11 + i) < t21)
      push_back(t11[i++]);
    _capacity = _size;
  }

