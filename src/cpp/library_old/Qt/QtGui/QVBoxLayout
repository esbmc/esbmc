#include "QTextEdit"
#include "QPushButton"

class QVBoxLayout : public QLayout {
public:
	QVBoxLayout (){}
	QVBoxLayout ( QWidget * parent ){}
	~QVBoxLayout (){}
	QVBoxLayout	addWidget ( QTextEdit * w ){}
	QVBoxLayout	addWidget ( QPushButton * w ){}
};
