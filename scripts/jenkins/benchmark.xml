  </tasks>
  <tasks name="Termination-Other">
    <includesfile>/data/sv-benchmarks/c/Termination-Other.set</includesfile>
    <propertyfile>/data/sv-benchmarks/c/properties/termination.prp</propertyfile>
  </tasks>
</rundefinition>

</benchmark>
